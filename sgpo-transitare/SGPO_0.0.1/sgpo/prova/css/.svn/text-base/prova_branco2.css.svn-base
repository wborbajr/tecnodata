/* CSS prova eletronica */

* {
	margin:0;
	padding:0;
	list-style:none;
}

body {
	font:11px Verdana, Arial, Helvetica, sans-serif;
	margin-top: 2px;
}

input {
	border: 0px;
}

/** GERAL **/
#geral {
	margin: auto;
	width:774px;
}

/** CLASSES **/

.titulo{
	font:bold 25px Verdana, Arial, Helvetica, sans-serif;
	color:#FFF;
}

.tela02tit{
	font:bold 25px Verdana, Arial, Helvetica, sans-serif;
	color:#E77817;
}

.tela02imput{
	border: 0px;
	background:#E77817;
}

.tela03tit{
	font:bold 20px Verdana, Arial, Helvetica, sans-serif;
