  `email` varchar(50) default NULL,
  `txtCargo` varchar(30) default NULL,
  `txtSenha` varchar(20) default NULL,
  `status` varchar(30) default NULL,
  `editar` enum('Sim','Não') default 'Não',
  `cadastrar` enum('Sim','Não') default 'Não',
  `excluir` enum('Sim','Não') default 'Não',
  `bnq` enum('Sim','Não') default 'Não',
  `bilhetagem` enum('Sim','Não') default 'Não',
  `clientes` enum('Sim','Não') default 'Não',
  `usuarios` enum('Sim','Não') default 'Não',
  `manutencao` enum('Sim','Não') default 'Não',
  `benchmark` enum('Sim','Não') default 'Não',
  `painel_cliente` enum('Sim','Não') default 'Não',
  `txtData` char(10) default NULL,
  `txtHora` char(10) default NULL,
  `txtIP` varchar(15) default NULL,
  PRIMARY KEY  (`codUsuario`),
  UNIQUE KEY `codUsuario` (`codUsuario`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

